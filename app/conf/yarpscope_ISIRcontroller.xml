<?xml version="1.0" encoding="UTF-8" ?>
<portscope rows="1" columns="1" carrier="tcp">
    <plot gridx="0"
        gridy="0"
        title="errors on COM tracking"
        size="60"
        minval="-0.2"
        maxval="0.2"
        bgcolor="LightSlateGrey">
        <graph remote="/comStepper/com_error:o"
        index="1"
        color="Red"
        title="Graph 0"
        type="lines"
        size="1" />
        <graph remote="/comStepper/com_error:o"
        index="2"
        color="Green"
        title="Graph 1"
        size="2" />
    </plot>
    <plot gridx="1"
        gridy="0"
        title="errors on R2L tracking"
        minval="-0.2"
        maxval="0.2"
        bgcolor="LightGrey">
        <graph remote="/comStepper/r2l_error:o"
        index="0"
        color="Red"
        title="Graph 0"
        type="lines"
        size="1" />
        <graph remote="/comStepper/r2l_error:o"
        index="1"
        color="Green"
        title="Graph 1"
        size="2" />
        <graph remote="/comStepper/r2l_error:o"
        index="2"
        color="Blue"
        title="Graph 2"
        size="3" />
    </plot>
</portscope>
